{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "learning-log.settings",
    "PYTHONPATH": "learning-log"
  },
  "build": {
    "command": "./build_files.sh"
  }
}
